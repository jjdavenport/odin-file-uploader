datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id Int @id @default(autoincrement())
    username String @unique
    password String
    uploads Upload[]
}

model Upload {
    id Int @id @default(autoincrement())
    file_name String
    created_at Int 
    userId Int
    user User @relation(fields: [userId], references: [id])
}